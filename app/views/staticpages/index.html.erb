<div class="col-md-12 text-md-center">
  <h1>GIF Minute</h1>
</div>

<div class="col-md-12 text-md-center">
  <% if current_user %>
    <div id="user_onboard_progress_div">
      <%= render partial: "user_onboard_progress", locals: { series: @series } %>
    </div>

    <%= image_tag("https://media.giphy.com/media/13bA2eQ0StNCAE/giphy.gif") %> <br> <br>


    <%= link_to "GO!", "#" %>
  <% else %>
    <%= link_to 'Step 1: Sign in With Twitter', '/auth/twitter' %>
  <% end %>
</div>
